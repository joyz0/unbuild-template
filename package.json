{
  "name": "unbuild-template",
  "version": "0.0.0",
  "description": "unbuild template project",
  "repository": "joyz0/unbuild-template",
  "homepage": "https://github.com/joyz0/unbuild-template",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "unpkg": "./dist/index.umd.js",
  "browser": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "unbuild": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "play": "node ./jiti.bootstrap.cjs",
    "dev": "vitest dev",
    "build": "unbuild",
    "prepack": "pnpm build",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "clean": "rimraf dist",
    "lint": "eslint . --cache --ext .ts,.js,.mjs,.cjs,.jsx,.tsx && prettier -c src test stories playground",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs,.jsx,.tsx . --fix && prettier -c src test stories playground -w",
    "stub": "pnpm unbuild --stub",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck",
    "ci": "pnpm install && pnpm lint && pnpm test",
    "release": "pnpm test && changelogen --release && git push --follow-tags && npm publish",
    "prerelease": "pnpm test && changelogen --prerelease --release && git push --follow-tags && npm publish --tag rc",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:clean": "rimraf storybook-static"
  },
  "keywords": [],
  "author": "joyz0",
  "license": "MIT",
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --cache --fix"
    ],
    "*.{css,less,scss}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "dompurify": "^3.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.25",
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@storybook/addon-essentials": "^8.0.2",
    "@storybook/addon-interactions": "^8.0.2",
    "@storybook/addon-links": "^8.0.2",
    "@storybook/addon-onboarding": "^8.0.2",
    "@storybook/blocks": "^8.0.2",
    "@storybook/react": "^8.0.2",
    "@storybook/react-vite": "^8.0.2",
    "@storybook/test": "^8.0.2",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "changelogen": "^0.5.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jiti": "^1.21.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "postcss-preset-env": "^9.5.1",
    "prettier": "^3.2.5",
    "prettier-plugin-two-style-order": "^1.0.1",
    "rimraf": "^5.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.0.2",
    "stylelint": "^16.2.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.2.0",
    "unbuild": "^2.0.0",
    "vite": "^5.2.2",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "@types/node": ">= 20",
    "typescript": "^5.4.2"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.4.0"
}
